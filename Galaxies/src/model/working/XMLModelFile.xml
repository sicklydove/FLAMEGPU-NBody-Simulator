<?xml version="1.0" encoding="utf-8"?>
<gpu:xmodel xmlns:gpu="http://www.dcs.shef.ac.uk/~paul/XMMLGPU"
        xmlns="http://www.dcs.shef.ac.uk/~paul/XMML">
  <name>Optional </name>
  <gpu:environment>
    <gpu:functionFiles>
      <file>functions.c</file>
    </gpu:functionFiles>
  </gpu:environment>
  <xagents>
    <gpu:xagent>
      <name>Particle</name>
      <memory>
        <gpu:variable>
          <type>int</type>
          <name>id</name>
        </gpu:variable>
      </memory>
      <functions>
        <gpu:function>
          <name>outputdata</name>
          <currentState>state1</currentState>
          <nextState>state1</nextState>
          <outputs>
            <gpu:output>
              <messageName>location</messageName>
              <gpu:type>single_message</gpu:type>
            </gpu:output>
          </outputs>
          <gpu:reallocate>false</gpu:reallocate>
          <gpu:RNG>false</gpu:RNG>
        </gpu:function>
      </functions>
      <states>
        <gpu:state>
          <name>state1</name>
        </gpu:state>
        <initialState>state1</initialState>
      </states>
      <gpu:type>continuous</gpu:type>
      <gpu:bufferSize>2048</gpu:bufferSize>
      <!---->
    </gpu:xagent>
  </xagents>
  <messages>
    <gpu:message>
      <name>location</name>
      <variables>
        <gpu:variable>
          <type>int</type>
          <name>id</name>
        </gpu:variable>
      </variables>
      <gpu:partitioningNone/>
      <gpu:bufferSize>2048</gpu:bufferSize>
    </gpu:message>

  </messages>
  <layers>
    <layer>
      <gpu:layerFunction>
        <name>outputdata</name>
      </gpu:layerFunction>
    </layer>
  </layers>
</gpu:xmodel>
